{
  "name": "@tidle-groupe/indigo",
  "version": "0.1.0",
  "description": "Un environnement de programmation léger avec rendu local et construction des fichiers pour la production",
  "main": "cmd/index.js",
  "bin": {
    "indigo": "bin/indigo"
  },
  "scripts": {
    "Build": "indigo build",
    "Run": "indigo run",
    "docker-start": "docker-compose -f indigo.yml start",
    "docker-stop": "docker-compose -f indigo.yml stop",
    "docker-install": "docker-compose -f indigo.yml up -d",
    "docker-uninstall": "docker-compose -f indigo.yml down"
  },
  "author": "Théo Laubezout",
  "license": "GPL-3.0",
  "repository": {
    "url": "git+https://github.com/Tidle-Groupe/indigo.git"
  },
  "dependencies": {
    "@node-minify/core": "^6.1.0",
    "@node-minify/csso": "^6.1.0",
    "@node-minify/html-minifier": "^6.1.0",
    "@node-minify/uglify-es": "^6.1.0",
    "chokidar": "^3.4.3",
    "fs": "0.0.1-security",
    "fs-extra": "^9.0.1",
    "readline": "^1.3.0"
  },
  "bugs": {
    "url": "https://github.com/Tidle-Groupe/indigo/issues"
  },
  "homepage": "https://github.com/Tidle-Groupe/indigo#readme",
  "devDependencies": {}
}
